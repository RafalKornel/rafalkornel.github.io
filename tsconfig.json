{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "./src/components/*"
      ],
      "@/layout/*": [
        "./src/layout/*"
      ],
      "@/lib/*": [
        "./src/lib/*"
      ]
    },
    "strictNullChecks": true,
    "allowJs": true
  },
}